{
  "name": "cqr-me",
  "version": "1.5.0",
  "description": "Have multiple env files in json/js that can be encrypted and included in version control",
  "main": "index.js",
  "bin": "./pkg/cli.js",
  "scripts": {
    "start": "node -r esm index.js",
    "lint": "eslint --fix -f compact --ignore-path .gitignore .",
    "test": "ava -v"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bernardoadc/cqr-me"
  },
  "keywords": [
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings",
    "process.env",
    "stages",
    "node_env",
    "development",
    "test",
    "production",
    "local",
    "encrypt",
    "secure-env",
    "encrypt-env",
    "decrypt-env"
  ],
  "author": "Bernardo A. Dal Corno",
  "license": "ISC",
  "dependencies": {
    "esm": "^3.2.25",
    "globby": "^11.0.3",
    "joi": "^17.4.2",
    "json5": "^2.2.0",
    "simple-encryptor": "^4.0.0"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  },
  "ava": {
    "files": [
      "tests/*"
    ]
  }
}
